<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--<declare-styleable name="ScrimInsetsView">
        <attr name="insetForeground" format="reference|color" />
    </declare-styleable>-->
    <declare-styleable name="CustomEditText">
        <attr name="edittextfont" format="string" />
        <attr name="edittextfontHint" format="string" />
        <attr name="edittextpwd" format="string" />
        <attr name="editcharpwd" format="string" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="shadow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="Theme"><attr name="circularImageViewStyle" format="reference"/></declare-styleable>


    <!-- Circulat Progress bar attrs-->
    <declare-styleable name="CircleProgress">
        <attr name="circle_progress" format="integer"/>
        <attr name="circle_max" format="integer"/>
        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>
        <attr name="circle_text_size" format="dimension"/>
        <attr name="circle_text_color" format="color"/>
        <attr name="circle_prefix_text" format="string"/>
        <attr name="circle_suffix_text" format="string"/>
    </declare-styleable>

</resources>